{
  "version": "1.1.7-beta.7",
  "id": "@shopgate/magento-user",
  "trusted": true,
  "configuration": {
    "registrationUrl": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.registrationUrl"
      }
    },
    "magentoUrl" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.magentoUrl"
      }
    },
    "credentials" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.credentials"
      }
    },
    "allowSelfSignedCertificate" : {
      "type": "bigApi",
      "destination" : "backend",
      "default": false,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.allowSelfSignedCertificate"
      }
    }
  },
  "components": [
    {
      "id": "RegisterSubscription",
      "path": "frontend/Register/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "CheckoutSubscription",
      "path": "frontend/Checkout/subscribers.js",
      "type": "subscribers"
    }
  ]
}

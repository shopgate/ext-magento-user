{
  "name": "@shopgate/magento-user",
  "version": "1.4.0-rc.1",
  "description": "prototype",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix --ignore-path ../.gitignore --ext .js --ext .jsx .",
    "test": "./node_modules/.bin/_mocha --use_strict --recursive ./test/unit",
    "cover": "./node_modules/.bin/nyc npm test && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "checkVulnerabilities": "check-audit",
    "newman": "./node_modules/.bin/newman run ./test/newman/collection.json --environment ./test/newman/environment.json --globals ./test/newman/globals.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shopgate/ext-magento-user.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "^4.17.5",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "mocha": "^5.2.0",
    "mocha-chai-helper": "1.0.0",
    "newman": "^4.2.2",
    "nock": "^9.0.13",
    "npm-audit-resolver": "^1.4.0",
    "nyc": "^13.1.0",
    "request-promise-native": "^1.0.5",
    "rewire": "^2.5.2",
    "sinon": "^5.0.10",
    "standard": "^11.0.1"
  },
  "standard": {
    "ignore": [],
    "env": {
      "mocha": true
    }
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "build",
      "docs",
      "coverage",
      "models/sgAppParameters",
      "models/utmParameters",
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "report-dir": "build"
  }
}

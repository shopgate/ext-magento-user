{
  "version": "1",
  "pipeline": {
    "id": "shopgate.user.addAddress.v1",
    "public": true,
    "input": [
      {"key": "address", "id": "1"}
    ],
    "output": [
      {"key": "success", "id": "2", "optional": true}
    ],
    "steps": [
      {
        "type": "pipeline",
        "id": "shopgate.general.getToken.v1",
        "trusted": true,
        "input": [],
        "output": [
          {"key": "token", "id": "10"}
        ]
      },
      {
        "type": "extension",
        "id": "@shopgate/magento-user",
        "path": "@shopgate/magento-user/user/getUser.js",
        "input": [
          {"key": "token", "id": "10"}
        ],
        "output": [
          {"key": "id", "id": "20"}
        ]
      },
      {
        "type": "extension",
        "id": "@shopgate/magento-user",
        "path": "@shopgate/magento-user/user/address/buildMagentoAddress.js",
        "input": [
          {"key": "address", "id": "1"}
        ],
        "output": [
          {"key": "magentoAddress", "id": "100"}
        ]
      },
      {
        "type": "extension",
        "id": "@shopgate/magento-user",
        "path": "@shopgate/magento-user/user/address/addTags.js",
        "input": [
          {"key": "address", "id": "1"},
          {"key": "magentoAddress", "id": "100"}
        ],
        "output": [
          {"key": "magentoAddress", "id": "100"}
        ]
      },
      {
        "type": "extension",
        "id": "@shopgate/magento-user",
        "path": "@shopgate/magento-user/user/addAddress_v1.js",
        "input": [
          {"key": "token", "id": "10"},
          {"key": "userId", "id": "20"},
          {"key": "magentoAddress", "id": "100"}
        ],
        "output": [
          {"key": "success", "id": "2",  "optional": true}
        ]
      }
    ]
  }
}
